{
  "name": "@agilo/medusa-plugin-bundles",
  "version": "1.0.0-alpha.2",
  "description": "A starter for Medusa projects.",
  "author": "Ivo Limic <ivo@agilo.co>",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "cross-env rimraf dist",
    "build": "cross-env yarn run clean && yarn run build:server && yarn run build:admin",
    "build:server": "cross-env yarn run clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "watch": "cross-env tsc --watch",
    "prepare": "cross-env NODE_ENV=production yarn run build:server && medusa-admin bundle",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env yarn run build && medusa start",
    "start:custom": "cross-env yarn run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env yarn run build:server && medusa develop"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "medusa-core-utils": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/admin": "^7.1.1",
    "@medusajs/medusa": "^1.16.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-portal": "^1.0.2",
    "@tanstack/react-query": "4.22.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react-table": "^7.7.9",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "medusa-interfaces": "^1.3.7",
    "medusa-react": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.38.0",
    "react-i18next": "^12.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.13.0",
    "react-table": "^7.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typeorm": "^0.3.16",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/admin": "^7.1.1",
    "@medusajs/medusa": "^1.16.0",
    "@tanstack/react-query": "4.22.0",
    "medusa-react": "^9.0.0",
    "qs": "^6.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.38.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.13.0",
    "typeorm": "^0.3.16"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ]
}
